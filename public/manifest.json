{
  "manifest_version": 3,
  "name": "Dummy-ext",
  "version": "0.0.1",
  "description": "This is a dummy extension",
  "options_page": "options.html",
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "FavIcon.png",
    "128": "FavIcon.png"
  },
  "action": {
    "default_icon": {
      "16": "logo.png",
      "32": "logo.png",
      "48": "FavIcon.png",
      "128": "FavIcon.png"
    }
  },
  "background": {
    "service_worker": "src/background.js",
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/contentScript/index.tsx"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab", "sidePanel"],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "web_accessible_resources": [{
    "resources": ["src/index.css"],
    "matches": ["<all_urls>"]
  }]
}
